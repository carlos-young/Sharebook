<h1>Edit Post</h1>

<%= form_for @post do |f| %>
<p> 
<%= f.label :name %><br />
<%= f.text_field :name %>
</p>
<p> 
<%= f.label :info %><br />
<%= f.text_area :info %>
</p>
<p>
  <%= f.submit "update Post" %>
</p>
<% end %>

<%= link_to 'Show', @post %> |
<%= link_to 'Back', posts_path %>
